        "eslint-import-resolver-babel-module": "5.3.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "3.4.1",
        "eslint-plugin-promise": "^6.0.0",
        "jest": "^29.0.3",
        "prettier-eslint": "13.0.0",
        "prettier-eslint-cli": "5.0.1"
    },
    "author": "Amplement",
    "license": "MIT"

        "eslint:fix": "eslint --fix .",
        "prettier:check": "prettier --list-different .",
        "prettier:fix": "prettier --write .",
        "test": "jest"
    },
    "dependencies": {
        "gelf-pro": "^1.3.8",
        "pm2": "^5.2.0",

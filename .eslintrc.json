{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": ["standard", "eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "settings": {
        "import/resolver": {
            "babel-module": {}
        }
    },
    "rules": {
        "no-console": "error"
    }
}
